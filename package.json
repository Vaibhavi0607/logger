{
  "name": "logger",
  "version": "1.0.0",
  "description": "Logger is a custom logger based on winston logging library. This library empowers projects to define their own format for printing the logs.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "start:dev": "nodemon -e ts -w ./src -x npm start",
    "test": "mocha './dist/*.spec.js'",
    "test:coverage": "tsc && nyc --reporter=html npm test",
    "lint": "eslint ./src/ --cache --ext .ts --fix",
    "pretty": "prettier --write src"
  },
  "repository": {
    "type": "git",
    "url": "\\"
  },
  "keywords": [
    "typescript",
    "node.js",
    "winston",
    "logger"
  ],
  "author": "Vaibhavi Ramani",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.3",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "sinon": "^12.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "dayjs": "^1.10.7",
    "nyc": "^15.1.0",
    "winston": "^3.4.0"
  }
}
